        url('../assets/fonts/SourceSans/SourceSansPro-BlackItalic.woff2') format('woff2'),
        url('../assets/fonts/SourceSans/SourceSansPro-BlackItalic.woff') format('woff'),
        url('../assets/fonts/SourceSans/SourceSansPro-BlackItalic.ttf') format('truetype')
    font-weight 900
    font-style italic
    font-display swap


