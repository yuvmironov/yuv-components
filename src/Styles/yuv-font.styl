@font-face {
  font-family: 'yuv-icon';
  src:
    url('../fonts/yuv-icon.ttf?ydscqn') format('truetype'),
    url('../fonts/yuv-icon.woff?ydscqn') format('woff'),
    url('../fonts/yuv-icon.svg?ydscqn#yuv-icon') format('svg');
  font-weight: normal;
  font-style: normal;
  font-display: block;
}

.icon {
  /* use !important to prevent issues with browser extensions that change fonts */
  font-family: 'yuv-icon' !important;
  speak: never;
  font-style: normal;
  font-weight: normal;
  font-variant: normal;
  text-transform: none;
  line-height: 1;

  /* Better Font Rendering =========== */
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

