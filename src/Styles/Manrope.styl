@font-face
    font-family 'Manrope'
    src url('../assets/fonts/Manrope/Manrope-ExtraLight.eot')
    src url('../assets/fonts/Manrope/Manrope-ExtraLight.eot?#iefix') format('embedded-opentype'),
        url('../assets/fonts/Manrope/Manrope-ExtraLight.woff2') format('woff2'),
        url('../assets/fonts/Manrope/Manrope-ExtraLight.woff') format('woff'),
        url('../assets/fonts/Manrope/Manrope-ExtraLight.ttf') format('truetype')
    font-weight 200
    font-style normal
    font-display swap


